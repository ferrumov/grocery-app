{
  "name": "expo-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "ios:dev": "APP_VARIANT=development expo start --ios",
    "ios:prod": "APP_VARIANT=production expo start --ios",
    "android:dev": "APP_VARIANT=development expo start --android",
    "android:prod": "APP_VARIANT=production expo start --android",
    "server:dev": "json-server --watch ./src/mock/db-dev.json --port 3000",
    "server:prod": "json-server --watch ./src/mock/db-prod.json --port 3000",
    "web": "expo start --web",
    "test:eslint": "expo lint",
    "test:tsc": "echo \"\\033[33mRunning typescript check\" && tsc",
    "lint": "eslint ."
  },
  "dependencies": {
    "@expo/html-elements": "latest",
    "@gluestack-style/react": "latest",
    "@gluestack-ui/config": "^1.1.5",
    "@gluestack-ui/themed": "latest",
    "@legendapp/motion": "latest",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@reduxjs/toolkit": "^2.2.7",
    "dayjs": "^1.11.13",
    "expo": "^51.0.8",
    "expo-status-bar": "~1.12.1",
    "formik": "^2.4.6",
    "json-server": "^1.0.0-beta.2",
    "lucide-react-native": "^0.441.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.6",
    "react-query": "^3.39.3",
    "react-query-devtools": "^2.6.3",
    "react-redux": "^9.1.2",
    "undefined": "@reduxjs/toolkit/query",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/webpack-config": "~19.0.1",
    "@types/react": "~18.2.45",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.1.2",
    "eslint-config-universe": "^13.0.0",
    "prettier": "^3.3.3",
    "typescript": "~5.3.3"
  },
  "private": true
}
